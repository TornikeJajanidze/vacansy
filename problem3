 public static int notContains(int[] Array){
        for (int j = 1; j<=Integer.MAX_VALUE; j++) {
            boolean temp = false;
            for (int i = 0; i < Array.length; i++) {
                if (j == Array[i]) {
                    temp = true;
                    break;
                }
            }
            if (!temp) {
                return j;
            }
        }
        return 0;
    }
